    "html-void-elements": "1.0.5",
    "ignore": "5.2.0",
    "jest-docblock": "27.5.1",
    "json5": "2.2.0",
    "leven": "3.1.0",
    "lines-and-columns": "2.0.3",
    "linguist-languages": "7.15.0",
    "lodash": "4.17.21",
    "mem": "9.0.2",
    "meriyah": "4.2.0",
    "micromatch": "4.0.4",
    "minimist": "1.2.5",
    "n-readlines": "1.0.1",
