    "postcss-values-parser": "2.0.1",
    "regexp-util": "1.2.2",
    "remark-footnotes": "2.0.0",
    "remark-math": "3.0.1",
    "remark-parse": "8.0.3",
    "resolve": "1.22.0",
    "semver": "7.3.5",
    "string-width": "5.0.1",
    "strip-ansi": "6.0.0",
